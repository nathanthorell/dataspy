# Database Connection Strings
# Copy this file to .env and fill in your actual connection details

# PostgreSQL
PG_DBCONN="host=localhost port=5432 user=postgres password=yourpassword dbname=postgres sslmode=disable"

# Microsoft SQL Server
MSSQL_DBCONN="server=localhost;user id=sa;password=yourpassword;port=1433;database=master"

# MySQL
MySQL_DBCONN="root:yourpassword@tcp(localhost:3306)/mysql"
